#include<iostream>

using namespace std;

struct nodo
{
	struct info;
	struct nodo *sgte;
}

nodo * crearLista()
{
	struct nodo *cabeza;
   	cabeza = new(struct nodo);
    	cabeza->sgte=NULL;
    	cabeza->ante=NULL;
    	return cabeza;   
 }

 cout<<"escriba numero de veces que desea aplicar la serie fibonacci":
 cin>>n;
 cout<<"escriba los 2 numeros que desea para la serie Fibonacci";cin>>info1>>info2;	
 
     for(i=0;i<n;i++)
    {
    	acum=info1+finfo2
	info1=info2
	info2=acum
	insertarFinal(cabeza,acum);
    
    
     }
void insertarFinal(nodo *cabeza, int valor)
{
	nodo *t, *q;
    cout<< "Entrada a insertar "<<valor;
	
	q = new(struct nodo);
    q->info  = valor;
    q->sgte = NULL;
    q->ante = NULL;
    
	t = cabeza;
	while(t->sgte!=NULL)
	{
		t = t->sgte;
	}
	t->sgte = q;
	q->ante=t;
}

    
