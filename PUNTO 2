#include<string.h>
#include<iostream>
#include<fstream>

using namespace std;

struct nodo
{
	string info;
	struct nodo *sgte;
	struct nodo *ante;
};

struct nodo *crearlis()
{
	struct nodo *lista;
	lista=NULL;
	return lista;
}
struct nodo *insertardesdeArchivo(struct nodo *lista)
{
	int infoArc;
	ifstream archivo;
	archivo.open("archivo lista.txt");
	if (archivo.is_open())
	{
	//	lista=crearlis();
		while(!archivo.eof())
		{
			archivo>>infoArc;
			lista=insertarinicio(lista,infoArc);
		}
		archivo.close();
	}
	else
	{
		cout<<"********   Archivo no encontrado   ******"<<endl;
	}
	return lista;
}
